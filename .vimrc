scriptencoding utf-8

set encoding=utf-8
scriptencoding utf-8
set fileencodings=utf-8,sjis

syntax enable

filetype plugin indent on

set cursorline

set number

set hlsearch

set ignorecase

set incsearch

set tabstop=2

set expandtab

set smartindent

set shiftwidth=2

set clipboard+=unnamed

set virtualedit=block

set wildmenu

set runtimepath+=~/src/github.com/sheerun/vim-polyglot

